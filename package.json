{
  "name": "eslint-config-standard-tunnckocore",
  "version": "0.0.0-semantic-release",
  "description": "Self preferences on top of StandardJS + JSX/React. In addition adds fp and jsdoc plugins. Requires Node.js >= 4",
  "repository": "tunnckoCore/eslint-config-standard-tunnckocore",
  "homepage": "https://github.com/tunnckoCore/eslint-config-standard-tunnckocore",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "nspId": "5397e94b-daba-4f18-a7a2-6f896ec3fd0b",
  "main": "./index.js",
  "license": "MIT",
  "scripts": {
    "fresh": "hela fresh",
    "contrib": "hela contrib",
    "renovate": "hela renovate",
    "lint": "hela lint -c .eslintrc.js",
    "test": "hela test node test.js",
    "precommit": "hela precommit",
    "commit": "hela commit",
    "postcommit": "git push",
    "semantic-release": "hela release"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=5"
  },
  "files": [
    "index.js"
  ],
  "dependencies": {
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsdoc": "3.7.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-standard": "3.1.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "hela": "2.0.10",
    "husky": "0.14.3",
    "semantic-release": "^7.0.0"
  },
  "keywords": [
    "charlike",
    "config",
    "es2015",
    "es2017",
    "es6",
    "ecmascript",
    "eslint",
    "hela",
    "javascript standard style",
    "jsx",
    "preference",
    "promise",
    "react",
    "standard",
    "standard style",
    "standardjs",
    "tunnckocore"
  ],
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "lines": 100
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "hela",
        "p-map-series",
        "execa",
        "mri"
      ],
      "highlight": "koa-better-router"
    },
    "reflinks": [
      "charlike-cli",
      "execa"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
